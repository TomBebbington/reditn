{
	"name": "Reditn",
	"version": "1.6.2",
	"manifest_version": 2,
	"description": "Reddit plugin that adds much-needed functionality such as image toggle buttons, advert blocking and more!",
	"icons": {
		"128": "icon.png"
	},
	"content_scripts": [
	{
		"matches": ["http://*.reddit.com/*"],
		"js": ["reditn.js"]
		}
	],
	"permissions": [
		"http://*.reddit.com/*",
		"http://www.xkcd.com/*",
		"http://www.explosm.net/comics/",
		"http://livememe.com/*",
		"http://backend.deviantart.com/*",
		"http://www.flickr.com/services/*",
		"http://open.ebay.com/*",
		"http://public-api.wordpress.com/rest/v1/*",
		"http://*.wikinews.org/w/*",
		"http://*.wikia.com/*",
		"https://api.github.com/*",
		"http://sourceforge.net/api/*",
		"https://api.twitter.com/*",
		"http://www.digitaltrends.com/*",
		"http://www.webupd8.com/*",
		"http://api.tumblr.com/v2/*",
		"https://api.imgur.com/3/*",
		"http://api.longurl.org/v2/*"
	]
}